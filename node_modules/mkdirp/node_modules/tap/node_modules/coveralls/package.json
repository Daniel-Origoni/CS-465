{
  "name": "coveralls",
  "description": "takes json-cov output into stdin and POSTs to coveralls.io",
  "version": "3.1.1",
  "keywords": [
    "coverage",
    "coveralls"
  ],
  "author": {
    "name": "Gregg Caines"
  },
  "license": "BSD-2-Clause",
